{
		"manifest_version": 2,
		"name": "Super Tab",
		"version": "0.0.1",
		"chrome_url_overrides": {
				"newtab": "newtab.html"
		},
		"permissions": [
			"topSites",
			"chrome://favicon/",
			"history",
			"tabs",
			"bookmarks",
			"storage"
		],
		"browser_action": {
			"default_icon": {
				"19": "push_19x19.png",
				"38": "push_38x38.png"
			},
			"default_title": "Push to SuperStack"
		},
		"background": {
			"scripts": ["background.js"]
		}
}
